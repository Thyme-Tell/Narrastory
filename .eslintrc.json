{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["eslint-config-airbnb-base", "plugin:prettier/recommended"],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-plusplus": 0,
        "import/no-unresolved": 0,
        "import/first": 0,
        "import/prefer-default-export": 0,
        "no-param-reassign": 0,
        "import/extensions": 0,
        "func-names":0
    }
}
